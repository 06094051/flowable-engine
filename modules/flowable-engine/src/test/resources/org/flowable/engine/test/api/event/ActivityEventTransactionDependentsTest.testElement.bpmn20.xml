<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns:flowable="http://flowable.org/bpmn"
             xmlns="http://www.omg.org/spec/BPMN/20100524/MODEL" targetNamespace="Examples">

    <process id="activityProcess">

        <extensionElements>
            <flowable:eventListener
                    class="org.flowable.engine.test.api.event.TestTransactionFlowableEventListener"
                    onTransaction="committed"
                    entityType="task"/>
        </extensionElements>

        <startEvent id="theStart"/>
        <sequenceFlow sourceRef="theStart" targetRef="userTask1"/>
        <userTask id="userTask1" name="userTask1"/>
        <sequenceFlow sourceRef="userTask1" targetRef="theEnd"/>

        <endEvent id="theEnd"/>

    </process>

</definitions>
